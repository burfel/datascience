%---------------CALIBRACAO RBS------------------
\subsection{RBS - Estequeometria de Óxidos \& Camada de $^{197}Au$ em detector de $^{28}Si$}
%\subsection{Calibração}

%\subsection{Estequiometria amostra óxidos}
\begin{frame}
  \frametitle{RBS}
  \framesubtitle{Calibração - amostra de Óxidos}
  
		\begin{figure}[H]
		\begin{minipage}{0.5\linewidth}
			\begin{centering}
			\hspace{-10pt}
			\includegraphics[width=\columnwidth]{../img/rbs_oxidos}
			  \vspace{-5pt}
			%\caption{Espectro RBS - Amostra de óxidos para diferentes condições de feixe de iões incidente}
			\end{centering}
		\end{minipage}%
		\begin{minipage}{0.5\linewidth}
			\begin{centering}
			\hspace{-10pt}
			\includegraphics[width=\columnwidth]{../img/calibracao_oxidos}
			  \vspace{-5pt}
			%\caption{Calibração Energia-Canal $E = p1 \cdot C + p0$ $(MeV)$ do sistema de detecção RBS (sessão 3)}
			\end{centering}
		\end{minipage}%
		\end{figure}

	%\vspace{+30pt}
	\begin{multicols*}{2}	
	%\vspace{-100pt}
	\footnotesize
	\begin{equation*}
	(O)_1(Si)_x(Er)_y(Ge)_z
	\end{equation*}
	\vspace{+1pt}
	\begin{equation*}
	H_{X}= \sigma_{X} \Omega Q \frac{N_X}{N^{comp}} \frac{E}{[\epsilon]_{X}^{Comp}}
	\end{equation*}
	\vspace{+3pt}
	\begin{equation*}
	\frac{H_{X}}{H_{Y}}= \frac{x}{y}\frac{\sigma_{X}}{\sigma_{Y}} \frac{[\epsilon]_{Y}^{Comp}}{[\epsilon]_{X}^{Comp}}
	\end{equation*}
	
	%\columnbreak

	\begin{table}[H]	
	\centering
	\vspace*{+5pt}
	\resizebox{0.9\columnwidth}{!}{\vspace{+100pt}\begin{tabular}{|c|c|c|c|}
	\hline
	\multicolumn{2}{|c|}{$E=p1\cdot C+p0$} & p1 (MeV/cnl) & p0 (MeV)\\ \hline
	\multicolumn{2}{|c|}{ } & $2.61 \times 10^{-3}$ & 0.1327\\ \hline
	\end{tabular}}
	\caption{\scriptsize Parâmetros de calibração	}
	\end{table}

	\begin{table}[H]	
	\centering
	\vspace*{-25pt}
	\resizebox{0.9\columnwidth}{!}{\vspace{+100pt}\begin{tabular}{|c|c|c|c|}
	\hline
	Feixe & x & y & z \\ \hline
	$^3He$ & $4.43\cdot 10^{-1}$  & $3.30\cdot 10^{-3}$ & $8.71 \cdot 10^{-2}$  \\ \hline \hline
	\multicolumn{2}{|c|}{Normalização (1/x)} & $7.43 \cdot 10^{-3}$ & $1.96\cdot 10^{-1}$ \\ \hline
	\multicolumn{2}{|c|}{Esperado (Docente)} & $8.00 \cdot 10^{-3}$ & $2.00\cdot 10^{-1}$ \\ \hline
	\end{tabular}}
	\caption{\scriptsize  Índices estequiométricos x,y,z relativos}
	\end{table}
	
	\normalsize
	\end{multicols*}
	
\end{frame}

%---------------DETETOR AU-SI-------------------
%\subsection{Análise Detetor $^{28}Si$ com filme de $^{197}Au$}
\begin{frame}

	\frametitle{RBS}
	\framesubtitle{Análise Detetor $^{28}Si$ com filme de $^{197}Au$ - Caracterização do Espetro}

	\begin{multicols*}{2}
	\begin{figure}[H]
	\centering
	\includegraphics[width=\columnwidth]{../img/rbs_au}
	\end{figure}
	\columnbreak

	\begin{itemize}
	\item Deslocamento do espetro (maior K para $^3He$)
	\item Maior número de contagens (maior $\sigma$ para $^3He$)
	\item $^{197} Au$ está à superfície
	\item Energias coincidem com as esperadas 
	\end{itemize}

	\end{multicols*}

	\vspace{-10pt}
	\begin{table}[H]
	\centering
	\resizebox{0.8\linewidth}{!}{
	\begin{tabular}{|c|c|c|c|c|c|c|c|}
	\hline
	Feixe    &   $\theta_1(^{\circ})$& $\bar{C}_{Au}$ (cnl) & $E_{Au}$ (MeV)  & $E_{Au_{teor}}$ (MeV) & $\bar{C}_{Si}$ (cnl)& $E_{Si}$ (MeV) & $E_{Si_{teor}}$ (MeV) \\ \hline\hline
	$^{4}He$    & 0  & 759       & 2.11 &2.12& 439     & 1.28 & 1.31\\ \hline
	$^{4}He$    & 45 & 759       & 2.11 &2.12& 439     & 1.28 & 1.31\\ \hline
	$^{3}He$    & 0  & 775          & 2.16&2.17 & 523     & 1.50& 1.51\\ \hline
	\end{tabular}}
	\caption{Valores de energia para patamares espetro}
	\end{table}
\end{frame}

