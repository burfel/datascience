%---------------CONCENTRAÇÃO 2H-------------------%
%\subsubsection{Concentração de $^2H$}
\begin{frame}
   \frametitle{NRA}
   \framesubtitle{Concentração $N_S$ de $^2H$}
	
	\begin{itemize}
		\item $\sigma \Rightarrow \frac{d\sigma}{d\Omega} = 9.815 \hspace{3pt} mbarn \hspace{3pt} sr^{-1}$\\
{\small $^3He$ a $2.3 MeV$ com $^2H$, ângulo $\theta=135^o$  (fonte: IBANDL)}
	\end{itemize}
	
	\begin{table}[H]
	\centering
	\begin{tabular}{|c|c|c|c|}\hline
	Amostra	& $A_S$ (cts) & Q (iões inc.) 	&	 $N_S$ (átm $cm^{-2}$)			\\ \hline \hline
	JET-A	& 3185 & $6.25\cdot 10^{13}$		&	1.39$\cdot 10 ^{17}$			\\ \hline
	JET-B	& 118490 & $6.25\cdot 10^{13}$	&	5.17$\cdot 10 ^{18}$			\\ \hline
	JET-C	& 2158 & $12.5\cdot 10^{13}$	&	4.71$\cdot 10 ^{16}$			\\ \hline
	\end{tabular}
	\caption{Concentrações $N_S$ de $^2H$ nas diferentes amostras do JET, calculadas através de $N_s = \frac{A_s}{Q \Omega \sigma}$}
	  \label{tab:concentraJET}
	\end{table}
	

\end{frame}

%---------------PROFUNDIDADE 2H------------------%
%\subsubsection{Profundidade de $^2H$}
\begin{frame}
	\frametitle{NRA}
	\framesubtitle{Profundidade de $^2H$ - $\frac{dE}{dx}$ por troços}	
	
	\vspace{-20pt}
	\begin{multicols*}{2}
		
		\vspace{8pt}
	   	\begin{equation*}
			\mathbf{ \Delta E = [s]t }
		\end{equation*}
		\begin{equation*}		
			\mathbf{ \Delta E_{^3He} = -\Delta E_{p_D} }
		\end{equation*}
		\vspace{-3pt}
		\begin{equation*}		
			\left(\frac{dE}{dx}\right)_{E_{p_D}}\simeq 0
		\end{equation*}
		\begin{equation*}
   			E_{(n+1)} = E_{n} - \left(\frac{dE}{dx}\right)_{E_{n}} \times h
   		\end{equation*}
   		\vspace{-5pt}
		\begin{equation*}
			h = 0.1  nm
		\end{equation*}   	
	   	\vspace{-5pt}
	   	\begin{equation*}   		
	   		E_N \leq E_0 - \Delta E_{p_D}
	   	\end{equation*}
	   	\vspace{-5pt}
	   	\begin{equation*}
			t={\Delta x}_{max} = Nh
	   	\end{equation*}	
		\vspace{-3pt}
		
	\columnbreak
	
	\vspace{-20pt}
	\begin{figure}[H]
	\hspace{-20pt}
	\includegraphics[width=1.2\columnwidth]{../img/spline.pdf}
	\end{figure}

	
	\vspace{-23pt}
	\begin{table}[H]
	\centering
		\scalebox{0.8}{	
		\begin{tabular}{|c|c|c|}\hline
		Amostras	&	$E_0-\Delta E_{p_D}$	&	$t$	\\
			&	(MeV)	&	($\mu$m)	\\ \hline
		JET-A	&	1.289	&	1.583	\\ \hline
		JET-B	&	1.261	&	1.622	\\ \hline
		JET-C	&	1.036	&	1.935	\\ \hline
		\end{tabular}
	}
	\caption{Profundidade calculada pelo método dE/dx por troços}
	\end{table}
	\end{multicols*}

\end{frame}

