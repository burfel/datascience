\section{Introdução}

%-------------RBS------------%
\subsection{RBS - Rutherford Backscattering Spectrometry} 

\begin{frame}
  \frametitle{RBS - Rutherford Backscattering Spectrometry}

  \begin{multicols}{2}
  \begin{figure}[H]
	\centering
	\includegraphics[width=\columnwidth]{../img/RBSesq}
  \end{figure}	

  \begin{itemize}
	\item Direto e não destrutivo
	\item Não influenciado por propriedades químicas, electrónicas, magnéticas, ópticas ou estruturais da amosta
	\item Baseado em cálculos absolutos , não recorre a quantificação de padrão
  \end{itemize}

  \end{multicols}
\end{frame}

\begin{frame}
  \frametitle{RBS}

  \framesubtitle{Factor de cinemática}
"Qual a espécie presente?"
  	\begin{figure}[H]
	\centering
	\includegraphics[width=.75\columnwidth]{../img/RBSesq1.pdf}
	\end{figure}
	
    \begin{equation*}
		k =\frac{E}{E_0}= \left(\frac{\sqrt{M^2-m^2sin^2(\theta)} + mcos(\theta)}{M+m}\right)^2
	\end{equation*}

\end{frame}

\begin{frame}
  \frametitle{RBS}
  \framesubtitle{Secção eficaz}
"Qual a concentração da espécie?"		
	\begin{figure}[H]
	\centering
	\includegraphics[width=.75\columnwidth]{../img/RBSesq2.pdf}
	\end{figure}	
	
	\vspace{-27pt}				
	
	\begin{equation*} \label{eq:dsigma}
		\frac{d\sigma}{d\Omega} (\theta) = \left(\frac{Z_mZ_Me^2}{4E}\right)^2
			\frac{4}{sin^4(\theta)}
			\frac{\left(\sqrt{1-\left(\frac{m}{M}sin(\theta)\right)^2}+cos(\theta)\right)^2}{\sqrt{1-\left(\frac{m}{M}						sin(\theta)			\right)^2}} \Rightarrow \sigma
	\end{equation*}
	
	\begin{equation*} \label{eq:Ns}
		A_s = N_s Q\Omega \sigma \Leftrightarrow N_s = \frac{A_s}{Q \Omega \sigma}
	\end{equation*}
	
\end{frame}

\begin{frame}
  \frametitle{RBS}
  \framesubtitle{Perda de energia}
	"Qual a profundidade da espécie?"
	\begin{figure}[H]
	\centering
	\includegraphics[width=.8\columnwidth]{../img/RBSesq3.pdf}
	\end{figure}

	\begin{equation*}\label{eq:delta_E}
\Delta E = \left(\frac{k}{cos(\theta_1)}\left.\frac{dE}{dx}\right|_{E_0} +
	                  \frac{1}{cos(\theta_2)}\left.\frac{dE}{dx}\right|_{KE_0}\right)t = [s]t
		\end{equation*}
	
\end{frame}

%-------------NRA------------%

\subsection{NRA  - Nuclear Reaction Analysis}

\begin{frame}
  \frametitle{NRA - Nuclear Reaction Analysis}  
\footnotesize

	\begin{multicols}{2}
	\begin{itemize}
		\item Não-destrutivo, independente do estado físico-químico, boa precisão		
		\item Adequado para espécie leve, substrato pesado
		\item $N_s = \frac{A_s}{Q \Omega \sigma}$ \& $\Delta E = [s] t$
		%\vspace{-15pt}
%\begin{centering}
%\begin{itemize}
		 \item ${^2H}({^3He},\alpha)p$\\
${^2H} + {^3He} \rightarrow \alpha + p $\\ ($p=p_D$)\\
		\vspace{10pt}
		 \item ${^9Be}({^3He},p)^{11}Be$\\
${^9Be} + {^3He} \rightarrow {^{11}Be} + p$ \\
 ($p=p_i$ ,  $i \in \{0,...,7\}$)\\

		%\end{itemize}
%	\end{itemize}
%\end{centering}
\end{itemize}

\normalsize	
\columnbreak
	\begin{table}[H]
	\centering
%\footnotesize
\tiny
		\begin{tabular}{c|c}\hline
		\#p	&	E (MeV) 	\\\hline
		$p_D$	&	12.280		\\\hline
		$p_0$	&	10.323		\\\hline
		$p_1$	&	8.198		\\\hline
		$p_2$	&	5.878		\\\hline
		$p_3$	&	5.303		\\\hline
		$p_4$	&	3.581		\\\hline
		$p_5$	&	3.531		\\\hline
		$p_6$	&	3.037		\\\hline
		$p_7$	&	2.345		\\\hline
		\end{tabular}
	\caption{\footnotesize Energias dos protões emitidos nas reações}
	\end{table}
\normalsize
	\begin{figure}[H]
	\begin{centering}
	\vspace{-10pt}
	\includegraphics[width=1\columnwidth]{../img/NRA_BE_AN}
	\vspace{-10pt}
	\end{centering}
	\end{figure}
	\end{multicols}
\normalsize
\end{frame}	

