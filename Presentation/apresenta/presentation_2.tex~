%\subsection{Detectores}

\begin{frame}
	\frametitle{Montagem experimental}
	\framesubtitle{Detectores utilizados}
	
	\begin{itemize}
		\item Detectores semicondutores (NRA \& RBS)
	\end{itemize}
	
	\begin{multicols*}{2}
		
		\begin{itemize}
			\item detector RBS
			
			\begin{itemize}
				\item $\theta = 165^o$
					\vspace{3pt}
				\item $\Omega_{RBS} = 11.4 msr$
					\vspace{3pt}
				\item $t_{activa} = 100 \mu m$			
			\end{itemize}						
						
		\end{itemize}
	
	\columnbreak
	
		\begin{itemize}
			\item detector NRA
			
			\begin{itemize}
				\item $\theta = 140^o$
					\vspace{3pt}
				\item $\Omega_{NRA} = 37.35 msr$					
					\vspace{3pt}
				\item $t_{activa} = 2 mm$
					\vspace{3pt}
				\item absorvedor à frente do detector
			\end{itemize}						
						
		\end{itemize}		
	
	\end{multicols*}	 		
	
	\vspace{-5pt}
	\begin{itemize}
		\item está associado a cada detector a cadeia eletrónica comum (pre-amp + amp + ADC), enviando o sinal digital para o software utilizado para análise
	\end{itemize}
\end{frame}


